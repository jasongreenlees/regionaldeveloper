{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/lullabot/custom_layouts.git",
        "no-api": true
        },
        {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.0",
        "drupal/animations": "^1.2",
        "drupal/asset_injector": "2.4",
        "drupal/bg_img_field": "^1.4",
        "drupal/blazy": "^2.0@RC",
        "drupal/block_class": "^1.0",
        "drupal/bootstrap_barrio": "^4.22",
        "drupal/captcha": "^1.0@beta",
        "drupal/claro": "^1.0@beta",
        "drupal/classy_paragraphs": "^1.0@beta",
        "drupal/codesnippet": "^1.6",
        "drupal/console": "^1.0.2",
        "drupal/field_group": "^1.0",
        "drupal/fixed_block_content": "^1.0",
        "drupal/honeypot": "^1.29",
        "drupal/layout_builder_modal": "^1.0@alpha",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.10",
        "drupal/paragraphs": "^1.10",
        "drupal/pathauto": "^1.5",
        "drupal/recaptcha": "^2.4",
        "drupal/views_bootstrap": "^4.1",
        "drupal/webform": "^5.5",
        "drush/drush": "^9.0.0",
        "lullabot/custom_layouts": "dev-master",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
