<?php

/**
 * Implements template_preprocess_hook().
 */
function compony_preprocess_node(&$variables, $hook) {
  // Create the possibility to use more specific preprocess function for more specific nodes.
  $function = __FUNCTION__ . '__' . $variables['node']->getType();
  if (function_exists($function)) {
    $function($variables, $hook);
  }
}
